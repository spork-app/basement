{
    "name": "spork/basement",
    "description": "A Spork Plugin",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Austin Kregel",
            "email": "5355937+austinkregel@users.noreply.github.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "spork/core": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Spork\\Basement\\": "src/",
            "Spork\\Basement\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spork\\Basement\\SporkServiceProvider"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spork\\Basement\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "guzzlehttp/guzzle": "^7.5",
        "mockery/mockery": "^1.5"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/pint"
    },
    "homepage": "https://github.com/spork-app/basement",
    "keywords": [
        "spork-plugins"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
